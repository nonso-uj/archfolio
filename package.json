{
  "name": "sas-portfolio",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@payloadcms/db-mongodb": "3.46.0",
    "@payloadcms/email-nodemailer": "3.46.0",
    "@payloadcms/next": "3.46.0",
    "@payloadcms/payload-cloud": "3.46.0",
    "@payloadcms/plugin-multi-tenant": "3.46.0",
    "@payloadcms/richtext-lexical": "3.46.0",
    "@payloadcms/ui": "3.46.0",
    "@radix-ui/react-label": "^2.1.7",
    "@tailwindcss/postcss": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.6",
    "fs": "0.0.1-security",
    "graphql": "^16.8.1",
    "lucide-react": "^0.525.0",
    "mailersend": "^2.6.0",
    "motion": "^12.23.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.5",
    "payload": "3.46.0",
    "payload-cloudinary": "2.0.0-alpha.3",
    "react": "19.1.0",
    "react-icons": "^5.5.0",
    "react-transition-progress": "^0.0.4",
    "sharp": "0.32.6",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
